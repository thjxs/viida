  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#2ac9ef"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#fda53d"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Keyword operator",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Storage modifier",
      "scope": ["storage.modifier.import", "storage.modifier.package"],
      "settings": {
        "foreground": "#535353"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#F9F1A5"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#0ad29a"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#0ad29a"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.other",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#0ad29a"
      }
    },
    {
      "name": "heading",
      "scope": "punctuation.definition.heading",
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "HTML declaration",
      "scope": [
        "text.html.basic meta.tag.sgml.html punctuation.definition.tag.html",
        "text.html.basic meta.tag.sgml.html meta.tag.sgml.doctype.html",
        "text.html.basic meta.tag.sgml.html meta.tag.sgml.doctype.html string.quoted.double.doctype.identifiers-and-DTDs.html"
      ],
      "settings": {
        "foreground": "#535353"
      }
    },
    {
      "name": "HTML character",
      "scope": [
        "text.html.basic constant.character.entity.html",
        "text.xml constant.character.entity.xml"
      ],
      "settings": {
        "foreground": "#0ad29a"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Tag start/end",
      "scope": [
        "meta.tag.start",
        "meta.tag.end",
        "punctuation.definition.tag.begin",
        "punctuation.definition.tag.end",
        "punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#72bfff"
      }
    },
    {
      "name": "CSS selector",
      "scope": [
        "meta.selector.css entity.name.tag.css",
        "meta.selector.css entity.other.attribute-name.class.css",
        "meta.selector.css entity.other.attribute-name.id.css",
        "meta.selector.css meta.attribute-selector.css entity.other.attribute-name.attribute.css",
        "meta.selector.css meta.attribute-selector.css string.unquoted.attribute-value.css"
      ],
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "CSS selector punctuation",
      "scope": "meta.selector.css meta.attribute-selector.css punctuation.separator.operator.css",
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "CSS selector status",
      "scope": [
        "meta.selector.css entity.other.attribute-name.pseudo-class.css",
        "meta.selector.css entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#e06c75"
      }
    },
    {
      "name": "CSS at-rule",
      "scope": [
        "keyword.control.at-rule",
        "meta.at-rule support.type.property-name"
